{
  "compilerOptions": {
    "module": "CommonJS",
    "target": "es6",
    "esModuleInterop": true,
    "noImplicitAny": true,
    "moduleResolution": "node",
    "sourceMap": true,
    "outDir": "dist",
    "baseUrl": ".",
    "paths": {
      "*": ["../node_modules/*"]
    },
    "typeRoots": ["../node_modules/@types"],
    "resolveJsonModule": true
  },
  "include": ["./seed.ts"]
}
